{
  "name": "jats-utils",
  "version": "1.0.13",
  "description": "Utility functions for working with JATS in Typescript",
  "author": "Rowan Cockett <rowan@curvenote.com>",
  "homepage": "https://github.com/curvenote/jats",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
