    "build:esm": "tsc",
    "build": "npm-run-all -l clean -p build:esm"
  },
  "bugs": {
    "url": "https://github.com/curvenote/jats/issues"
  },
  "dependencies": {
    "unist-util-select": "^4.0.0"
  },
  "peerDependencies": {
    "xml-js": "^1",
    "jats-tags": "^1.0.13"
  },
  "devDependencies": {
    "myst-common": "^1.0.0",
    "jats-tags": "^1.0.13"
  }
}
